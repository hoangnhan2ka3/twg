{
    "name": "twg",
    "version": "1.0.0-beta.3",
    "description": "A utility function for grouping TailwindCSS variants.",
    "author": "hoangnhan2ka3",
    "homepage": "https://github.com/hoangnhan2ka3/twg",
    "repository": {
        "type": "git",
        "url": "https://github.com/hoangnhan2ka3/twg"
    },
    "bugs": {
        "url": "https://github.com/hoangnhan2ka3/twg/issues"
    },
    "packageManager": "pnpm@9.7.1+sha256.46f1bbc8f8020aa9869568c387198f1a813f21fb44c82f400e7d1dbde6c70b40",
    "engines": {
        "node": ">=6"
    },
    "scripts": {
        "build": "tsup",
        "test": "jest",
        "lint": "tsc",
        "npm": "pnpm build && pnpm publish --access public"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./replacer": {
            "require": "./dist/replacer/index.js",
            "import": "./dist/replacer/index.mjs",
            "types": "./dist/replacer/index.d.ts"
        },
        "./lite": {
            "require": "./dist/lite/index.js",
            "import": "./dist/lite/index.mjs",
            "types": "./dist/lite/index.d.ts"
        },
        "./lite/replacer": {
            "require": "./dist/lite/replacer/index.js",
            "import": "./dist/lite/replacer/index.mjs",
            "types": "./dist/lite/replacer/index.d.ts"
        }
    },
    "devDependencies": {
        "@eslint/compat": "^1.1.1",
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.9.0",
        "@stylistic/eslint-plugin": "^2.6.2",
        "@types/eslint": "^9.6.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.3.0",
        "@typescript-eslint/eslint-plugin": "^8.1.0",
        "@typescript-eslint/parser": "^8.1.0",
        "eslint": "^9.9.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-import-newlines": "^1.4.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "jest": "^29.7.0",
        "terser": "^5.31.6",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "tsup": "^8.2.4",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.1.0"
    },
    "keywords": [
        "tailwindcss",
        "tailwind",
        "tailwind variants grouping",
        "grouping tailwind variants",
        "variants grouping",
        "grouping variants",
        "tvg",
        "twg",
        "clsx",
        "classnames"
    ],
    "license": "MIT"
